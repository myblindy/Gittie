<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Gittie.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Gittie"
    Shell.FlyoutBehavior="Flyout"
    ParentChanged="ShellParentChanged">

    <ShellContent Title="Open" Route="OpenPage"
                  ContentTemplate="{DataTemplate local:OpenPage}"/>

    <ShellContent Title="Repository" Route="RepositoryPage"
                  ContentTemplate="{DataTemplate local:RepositoryPage}"/>

</Shell>
